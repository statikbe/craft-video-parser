{
    "name": "statikbe/craft-video-parser",
    "description": "Parse youtube & vimeo url's to make embeds",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "video-embed"
    ],
    "support": {
        "docs": "https://github.com/statikbe/craft-video-parser/blob/master/README.md",
        "issues": "https://github.com/statikbe/craft-video-parser/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Statik",
            "homepage": "https://www.statik.be"
        },
        {
            "name": "Jan Henckens",
            "hompage": "https://github.com/janhenckens/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "statikbe\\videoparser\\": "src/"
        }
    },
    "extra": {
        "name": "Video Parser",
        "handle": "video-parser",
        "schemaVersion": "1.0.0",
        "hasSettings": false,
        "developer": "Statik",
        "class": "statikbe\\videoparser\\VideoParser"
    },
    "require-dev": {
        "codeception/codeception": "^4.0.0",
        "vlucas/phpdotenv": "^3.0",
        "codeception/module-yii2": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    }
}
